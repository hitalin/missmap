{
	"name": "missmatch",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:cf": "pnpm build && wrangler pages dev .svelte-kit/cloudflare --d1=DB --kv=CACHE",
		"build": "vite build",
		"preview": "pnpm build && wrangler pages dev .svelte-kit/cloudflare --d1=DB --kv=CACHE",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:init": "wrangler d1 execute DB --local --file=migrations/0001_init.sql"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251225.0",
		"@sveltejs/adapter-cloudflare": "^6.0.0",
		"@sveltejs/kit": "^2.21.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.0.0",
		"wrangler": "^4.54.0"
	},
	"dependencies": {
		"cytoscape": "^3.30.0"
	}
}
